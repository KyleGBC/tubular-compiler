IF if
ELSE else
WHILE while
RETURN return
BREAK break
CONTINUE continue
SQRT sqrt
FUNCTION function
TYPE "int"|"char"|"double"|"string"
LITERAL_STRING \"([^\n"\\]|(\\[n"\\]))*\"
LITERAL_DOUBLE \d+\.(\d+)?|(\d+)?\.\d+
LITERAL_INT \d+
LITERAL_CHAR \'[^\\'\n\t]\'|\'\\.\'
LESS_EQUAL <=
GREATER_EQUAL >=
EQUAL ==
INEQUAL !=
LOGICAL_OR \|\|
LOGICAL_AND &&
IDENT \w+
-COMMENT //.*
-WHITESPACE \s+
